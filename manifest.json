{
    "name": "Conscient",
    "version": "1.0",
    "manifest_version": 2,
    "description": "Build an Extension!",
    "permissions": ["declarativeContent","storage","activeTab","audio","input"],
    "background": {
      "scripts": ["background.js"],
      "persistent": false
    },
   
    "icons": {

      "16": "logo/conscient_logo_16.png",
      "48": "logo/conscient_logo_48.png",
      "128": "logo/conscient_logo_128.png"
    },
    "browser_action":{
        "default_title":"Conscient",
        "default_popup":"popup.html"
    }

  }